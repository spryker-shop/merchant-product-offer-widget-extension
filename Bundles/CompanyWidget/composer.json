{
  "name": "spryker-shop/company-widget",
  "type": "library",
  "description": "CompanyWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/customer": "^7.0.0",
    "spryker/kernel": "^3.30.0"
  },
  "require-dev": {
    "spryker-shop/shop-ui": "*",
    "spryker/code-sniffer": "*"
  },
  "suggest": {
    "spryker-shop/shop-ui": "To add widget customer navigation menu: ^1.28.1."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
